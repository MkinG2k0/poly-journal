{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "src",
    "rootDir": "src",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "outDir": "./dist/",
    "jsx": "react-jsx",
    "declaration": true,
    "paths": {
      //      "@helper": [
      //        "Helper/helper"
      //      ],
      //      "@comp/*": [
      //        "Component/*"
      //      ],
      //      "@lib/*": [
      //        "Lib/*"
      //      ],
      //      "Hook/*": [
      //        "Hook//*"
      //      ],
      //      "@style/*": [
      //        "Style/*"
      //      ],
      //      "@interface/*": [
      //        "Interface/*"
      //      ]
    }
  },
  "include": [
    "*.d.ts",
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
